    <!--  Graduation Button -->
    <Button
        android:id="@+id/btnCloud"
        style="@style/DashboardButton"
        android:drawableTop="@drawable/ic_cloud"
        android:text="@string/nav_cloud" />

    <!--  Sharing Button -->
    <Button
        android:id="@+id/btnSharing"
        style="@style/DashboardButton"
        android:drawableTop="@drawable/ic_share_black_24dp"
        android:text="Sharing" />


</com.expansion.lg.kimaru.expansion.other.DashboardLayout>
