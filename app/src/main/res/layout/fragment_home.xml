    <!--  Training Button -->
    <Button
        android:id="@+id/btnTraining"
        style="@style/DashboardButton"
        android:drawableTop="@drawable/ic_training"
        android:text="@string/nav_training" />

    <!--  Graduation Button -->
    <Button
        android:id="@+id/btnGraduation"
        style="@style/DashboardButton"
        android:drawableTop="@drawable/ic_graduation"
