    <!--  Mapping Button -->
    <Button
        android:id="@+id/btnMapping"
        style="@style/DashboardButton"
        android:drawableTop="@drawable/ic_map_black_24dp"
        android:text="Mapping" />

    <!--  Recruitments Button -->
    <Button
        android:id="@+id/btnRecruitments"
        style="@style/DashboardButton"
        android:drawableTop="@drawable/ic_photo_library_black_24dp"
        android:text="Recruitments" />

    <!--  Registrations Button -->
    <Button
        android:id="@+id/btnRegistrations"
        style="@style/DashboardButton"
        android:drawableTop="@drawable/ic_registrations_24dp"
        android:text="Registrations" />

    <!--  Exams Button -->
   <Button
        android:id="@+id/btnExams"
        style="@style/DashboardButton"
        android:drawableTop="@drawable/ic_exam_black_24dp"
        android:text="Exams" />

    <!--  Interviews Button -->

    <Button
        android:id="@+id/btnInterviews"
        style="@style/DashboardButton"
        android:drawableTop="@drawable/ic_local_library_black_24dp"
